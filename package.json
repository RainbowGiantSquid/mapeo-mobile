{
  "name": "dd-reporter-app",
  "version": "0.1.0",
  "description": "A reporter app made for empowerment by Digital Democracy",
  "main": "main.js",
  "keywords": [
    "reporter",
    "i_dont_really_know"
  ],
  "author": "Tiago Fassoni",
  "license": "MPL-2.0",
  "browserify": {
    "transform": [
      "envify",
      [
        "reactify",
        {
          "stripTypes": true
        }
      ],
      "uglifyify",
      "babelify"
    ]
  },
  "scripts": {
    "budo": "NODE_ENV=development budo main.js --live | garnish",
    "build": "NODE_ENV=production browserify main.js | uglifyjs -cm > dist/bundle.js",
    "flow": "flow check",
    "lint": "standard",
    "start": "npm run watch & npm run serve",
    "watch": "watchify main.js -o dist/bundle.js -dv",
    "serve": "ecstatic -p 8000 dist",
    "test": "tape tests/*.test.js | tap-spec",
    "test-browser": "browserify test/*.js | testlingify"
  },
  "dependencies": {
    "babelify": "^6.3.0",
    "browserify": "^10.2.6",
    "ecstatic": "~0.8.0",
    "immutable": "^3.7.5",
    "immutable-js": "^0.3.1-6",
    "ramda": "^0.17.1",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "react-redux": "^3.0.1",
    "react-router": "^1.0.0-rc1",
    "redux": "^3.0.2",
    "redux-immutablejs": "0.0.6",
    "uglify-js": "^2.4.24",
    "uglifyify": "^3.0.1",
    "watchify": "^3.2.3"
  },
  "devDependencies": {
    "babel-tape-runner": "^1.2.0",
    "budo": "^5.1.1",
    "envify": "^3.4.0",
    "eslint": "^1.6.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "eslint-plugin-standard": "^1.3.1",
    "garnish": "^3.2.1",
    "reactify": "^1.1.1",
    "redux-devtools": "^2.1.5",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.1"
  }
}
