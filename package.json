{
  "name": "dd-reporter-app",
  "version": "0.1.0",
  "description": "A reporter app made for empowerment by Digital Democracy",
  "main": "main.js",
  "keywords": [
    "reporter",
    "i_dont_really_know"
  ],
  "author": "Tiago Fassoni",
  "license": "MPL-2.0",
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "scripts": {
    "boring-test": "babel-tape-runner tape tests/*.test.js | tap-spec",
    "build": "NODE_ENV=production browserify main.js | uglifyjs -cm > dist/bundle.js",
    "debug-test": "babel-tape-runner tape tests/*.test.js | tap-bail | tap-spec",
    "flow": "flow check",
    "lint": "standard",
    "start": "NODE_ENV=development budo main.js -- -p livereactload | garnish",
    "test": "babel-tape-runner tape tests/*.test.js | tap-nyan",
    "test-browser": "browserify test/*.js | testlingify"
  },
  "dependencies": {
    "camelcase": "^1.2.1",
    "history": "^1.12.6",
    "id-geo": "^1.0.0-alpha1",
    "immutable": "^3.7.5",
    "immutable-js": "^0.3.1-6",
    "invariant": "^2.1.2",
    "keymirror": "^0.1.1",
    "material-ui": "^0.13.1",
    "ramda": "^0.17.1",
    "react": "^0.14.0",
    "react-addons-create-fragment": "^0.14.0",
    "react-addons-pure-render-mixin": "^0.14.0",
    "react-addons-transition-group": "^0.14.0",
    "react-addons-update": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-infinite": "^0.5.10",
    "react-redux": "^3.1.0",
    "react-router": "^1.0.0-rc3",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.0.2",
    "redux-immutablejs": "0.0.7",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "uuid": "^2.0.1",
    "watchify": "^3.4.0"
  },
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-eslint": "^4.1.3",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-tape-runner": "^1.2.0",
    "babelify": "^6.3.0",
    "browserify": "^12.0.1",
    "budo": "^5.1.1",
    "deep-equal": "^1.0.1",
    "envify": "^3.4.0",
    "eslint": "^1.6.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "eslint-plugin-standard": "^1.3.1",
    "garnish": "^3.2.1",
    "livereactload": "^2.0.5",
    "proxyquire": "^1.7.3",
    "react-proxy": "^1.1.1",
    "reactify": "^1.1.1",
    "redux-devtools": "^2.1.5",
    "redux-logger": "^2.0.3",
    "redux-promise": "^0.5.0",
    "smash": "0.0.14",
    "standard": "^5.3.1",
    "tap-nyan": "0.0.2",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.1",
    "uglify-js": "^2.4.24",
    "uglifyify": "^3.0.1"
  }
}
