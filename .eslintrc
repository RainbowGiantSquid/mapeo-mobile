{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended",
    "prettier",
  ],
  "parser": "babel-eslint",
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "import/no-extraneous-dependencies": ["off"],
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
